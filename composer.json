{
  "name": "bytic/dispatcher",
  "type": "library",
  "description": "dispatcher bytic framework",
  "homepage": "https://github.com/bytic/dispatcher",
  "license": "MIT",
  "authors": [
    {
      "name": "Gabriel Solomon",
      "email": "hello@gabrielsolomon.ro"
    }
  ],
  "autoload": {
    "psr-4": {
      "Nip\\Dispatcher\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Nip\\Dispatcher\\Tests\\": "tests/src",
      "Modules\\": "tests/fixtures/modules"
    }
  },
  "require": {
    "php": "^7.4|^8.0",
    "bytic/container": "^0.9|^1.0|^2.0",
    "bytic/collections": "^0.9|^1.0|^2.0",
    "bytic/http": "^0.9|^1.0",
    "bytic/inflector": "^0.9|^1.0",
    "bytic/pipeline": "^1.0"
  },
  "require-dev": {
    "bytic/phpqatools": "^1.0",
    "bytic/controllers": "^0.9|^1.0"
  },
  "suggest": {
    "bytic/controllers": "^1.0"
  },
  "prefer-stable": true,
  "minimum-stability": "dev",
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "ergebnis/composer-normalize": true,
      "phpstan/extension-installer": true,
      "bytic/phpqatools": true
    }
  }
}
